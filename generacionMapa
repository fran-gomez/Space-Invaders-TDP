areaAliensH = mapHeight / 3;
areaAliensW = mapWidth;
cuadradoAlienW = areaAliensW / cantEnemigosXFila;
cuadradoAlienH = areaAliensH / cantFilasEnemigos;

for f = 0 to cantFilasEnemigos
	for c = 0 to cantEnemigosXFila) 
		enemigo <- new Enemigo
		x <- c*cuadradoAlienW + cuadradoAlienW / 2
		y <- f*cuadradoAlienH + cuadradoAlienH / 2
		e.setUbicacion(x,y);
		panel.añadir(e)

jugador = new ("Spaceship1");
jugador.setLocation(posJugadorX, posJugadorY);

